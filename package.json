{
  "name": "@middlender/with-memo",
  "version": "0.0.1",
  "description": "High order function for memoization",
  "main": "dist/with-memo.js",
  "module": "dist/with-memo.mjs",
  "types": "dist/with-memo.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "tsup src/*.ts src/**/*.ts --format cjs,esm --dts",
    "lint": "eslint --fix --ext .ts --max-warnings 0 src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KnightSlayer/with-memo.git"
  },
  "keywords": [
    "memo",
    "cahce",
    "memoization",
    "HoF"
  ],
  "author": "Edgar Abgaryan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KnightSlayer/with-memo/issues"
  },
  "homepage": "https://github.com/KnightSlayer/with-memo#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.28.4",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  }
}
